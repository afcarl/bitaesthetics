---
layout: base
---
div
    each project in getCollection('projects').toJSON()
        div(class="project")
            h2 #{project.name}
            div(class="row container")
                div(class='half')
                    !{formatDate(project.date, 'MMMM YYYY')}
                div(class='half rightalign')
                    !{project.format}
            p !{project.contentRendered}
